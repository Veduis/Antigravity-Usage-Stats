{
  "name": "antigravity-usage-stats",
  "displayName": "Antigravity Usage Stats",
  "description": "Monitor AI model usage quotas in VS Code with status bar indicators and quick access",
  "version": "0.2.0",
  "publisher": "Veduis",
  "license": "MIT",
  "icon": "icon.png",
  "homepage": "https://veduis.com/blog/antigravity-usage-stats",
  "repository": {
    "type": "git",
    "url": "https://github.com/Veduis/Antigravity-Usage-Stats"
  },
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other",
    "Visualization"
  ],
  "keywords": [
    "ai",
    "quota",
    "dashboard",
    "monitoring",
    "antigravity"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "antigravityStats.showQuotas",
        "title": "Show Quotas",
        "category": "Antigravity Usage Stats"
      },
      {
        "command": "antigravityStats.refresh",
        "title": "Refresh Quota Data",
        "category": "Antigravity Usage Stats"
      },
      {
        "command": "antigravityStats.openLogs",
        "title": "Open Logs",
        "category": "Antigravity Usage Stats"
      },
      {
        "command": "antigravityStats.pinModel",
        "title": "Pin Model to Status Bar",
        "category": "Antigravity Usage Stats"
      },
      {
        "command": "antigravityStats.unpinModel",
        "title": "Unpin Model from Status Bar",
        "category": "Antigravity Usage Stats"
      }
    ],
    "keybindings": [
      {
        "command": "antigravityStats.showQuotas",
        "key": "ctrl+shift+q",
        "mac": "cmd+shift+q"
      }
    ],
    "configuration": {
      "title": "Antigravity Usage Stats",
      "properties": {
        "antigravityStats.refreshInterval": {
          "type": "number",
          "default": 120,
          "minimum": 10,
          "maximum": 3600,
          "description": "Seconds between auto-refreshes"
        },
        "antigravityStats.statusBarFormat": {
          "type": "string",
          "default": "dot-percent",
          "enum": [
            "icon",
            "dot",
            "percent",
            "dot-percent",
            "name-percent",
            "full"
          ],
          "description": "Format of the status bar display"
        },
        "antigravityStats.groupingEnabled": {
          "type": "boolean",
          "default": true,
          "description": "Group models by quota pools in QuickPick view"
        },
        "antigravityStats.warningThreshold": {
          "type": "number",
          "default": 30,
          "minimum": 0,
          "maximum": 100,
          "description": "Yellow alert threshold (%)"
        },
        "antigravityStats.criticalThreshold": {
          "type": "number",
          "default": 10,
          "minimum": 0,
          "maximum": 100,
          "description": "Red alert threshold (%)"
        },
        "antigravityStats.notificationsEnabled": {
          "type": "boolean",
          "default": true,
          "description": "Show quota alerts"
        },
        "antigravityStats.pinnedModels": {
          "type": "array",
          "default": [],
          "items": {
            "type": "string"
          },
          "description": "Model names pinned to the status bar (e.g., ['Claude 3.5 Sonnet', 'Gemini 3 Pro (High)'])"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "build": "node esbuild.config.js",
    "build:watch": "node esbuild.config.js --watch",
    "lint": "eslint src --ext ts",
    "lint:fix": "eslint src --ext ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "node ./out/test/runTest.js",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/vscode": "^1.90.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "esbuild": "^0.19.8",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2",
    "@vscode/vsce": "^2.22.0"
  },
  "dependencies": {}
}