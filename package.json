{
  "name": "antigravity-stats",
  "displayName": "Antigravity Stats",
  "description": "A customizable dashboard for monitoring AI model usage quotas in VS Code",
  "version": "0.1.0",
  "publisher": "veduis",
  "repository": {
    "type": "git",
    "url": "https://github.com/veduis/antigravity-stats"
  },
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other",
    "Visualization"
  ],
  "keywords": [
    "ai",
    "quota",
    "dashboard",
    "monitoring",
    "antigravity"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "antigravityStats.openDashboard",
        "title": "Open Dashboard",
        "category": "Antigravity Stats"
      },
      {
        "command": "antigravityStats.refresh",
        "title": "Refresh Quota Data",
        "category": "Antigravity Stats"
      },
      {
        "command": "antigravityStats.openLogs",
        "title": "Open Logs",
        "category": "Antigravity Stats"
      },
      {
        "command": "antigravityStats.switchMode",
        "title": "Switch Display Mode",
        "category": "Antigravity Stats"
      },
      {
        "command": "antigravityStats.configureAutoWake",
        "title": "Configure Auto Wake",
        "category": "Antigravity Stats"
      },
      {
        "command": "antigravityStats.selectStatusBarModels",
        "title": "Select Status Bar Models",
        "category": "Antigravity Stats"
      }
    ],
    "keybindings": [
      {
        "command": "antigravityStats.openDashboard",
        "key": "ctrl+shift+q",
        "mac": "cmd+shift+q"
      }
    ],
    "configuration": {
      "title": "Antigravity Stats",
      "properties": {
        "antigravityStats.displayMode": {
          "type": "string",
          "default": "webview",
          "enum": [
            "webview",
            "quickpick"
          ],
          "enumDescriptions": [
            "Visual dashboard in a webview panel",
            "Keyboard-friendly QuickPick list"
          ],
          "description": "Display mode for the quota dashboard"
        },
        "antigravityStats.refreshInterval": {
          "type": "number",
          "default": 120,
          "minimum": 10,
          "maximum": 3600,
          "description": "Seconds between auto-refreshes"
        },
        "antigravityStats.statusBarFormat": {
          "type": "string",
          "default": "dot-percent",
          "enum": [
            "icon",
            "dot",
            "percent",
            "dot-percent",
            "name-percent",
            "full"
          ],
          "description": "Format of the status bar display"
        },
        "antigravityStats.groupingEnabled": {
          "type": "boolean",
          "default": true,
          "description": "Auto-group models by quota pools"
        },
        "antigravityStats.warningThreshold": {
          "type": "number",
          "default": 30,
          "minimum": 0,
          "maximum": 100,
          "description": "Yellow alert threshold (%)"
        },
        "antigravityStats.criticalThreshold": {
          "type": "number",
          "default": 10,
          "minimum": 0,
          "maximum": 100,
          "description": "Red alert threshold (%)"
        },
        "antigravityStats.notificationsEnabled": {
          "type": "boolean",
          "default": true,
          "description": "Show quota alerts"
        },
        "antigravityStats.statusBarModels": {
          "type": "array",
          "default": [],
          "items": {
            "type": "string"
          },
          "description": "Model names to display individually in the status bar (e.g., ['Claude 3.5 Sonnet', 'Gemini 3 Pro (High)'])"
        },
        "antigravityStats.pinnedModels": {
          "type": "array",
          "default": [],
          "items": {
            "type": "string"
          },
          "description": "Models to pin to the top of the dashboard"
        },
        "antigravityStats.autoWakeSchedule": {
          "type": "string",
          "default": "",
          "description": "Cron expression for auto wake-ups"
        },
        "antigravityStats.themeOverrides": {
          "type": "object",
          "default": {},
          "description": "Custom color overrides (e.g., { \"progressGreen\": \"#00ff00\" })"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "build": "node esbuild.config.js",
    "build:watch": "node esbuild.config.js --watch",
    "lint": "eslint src --ext ts",
    "lint:fix": "eslint src --ext ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "node ./out/test/runTest.js",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/vscode": "^1.90.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "esbuild": "^0.19.8",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2",
    "@vscode/vsce": "^2.22.0"
  },
  "dependencies": {}
}